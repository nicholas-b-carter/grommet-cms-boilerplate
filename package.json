  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/config/testing/preprocessor.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ]
  },
  "dependencies": {
