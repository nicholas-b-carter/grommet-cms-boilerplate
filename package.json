    "imports-loader": "^0.6.3",
    "react-router-scroll": "^0.3.1",
    "react-scroll": "^1.2.0",
    "remove-flow-types-loader": "^1.0.0",
    "url-loader": "^0.5.7",
    "uuid": "^3.0.1",
    "webpack": "^1.9.11",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-stream": "^3.2.0"
  },
  "scripts": {
    "test": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=test node --harmony node_modules/.bin/jest",
    "test:update": "npm run test -- -u",
    "test:watch": "npm run test -w",
    "build": "gulp dist",
    "dev": "gulp dev",
    "dev:alias": "gulp dev --useAlias",
    "start": "node server/babel-server.js",
    "prod-start": "NODE_ENV='production' pm2 start server/babel-server.js -i 0 --name grommet-cms",
    "prod-stop": "pm2 stop grommet-cms",
    "generator": "plop --plopfile ./config/generators/cli.js",
    "generate": "npm run generator",
    "generate:component": "plop --plopfile ./config/generators/index.js component",
    "generate:container": "plop --plopfile ./config/generators/index.js container",
    "setup": "flow-typed install && npm install"
  }
}
