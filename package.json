    "remarkable": "^1.7.0",
    "request": "^2.75.0",
    "reselect": "^2.5.4",
    "sinon": "^1.17.7"
  },
  "devDependencies": {
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "classnames": "^2.2.5",
